	<wsp:Policy wsu:Id="CustomBinding_IWSTrustFeb2005Async_policy">
		<wsp:ExactlyOne>
			<wsp:All>
				<msis:DomainInternet/>
				<http:NegotiateAuthentication/>
				<sp:TransportBinding>
					<wsp:Policy>
